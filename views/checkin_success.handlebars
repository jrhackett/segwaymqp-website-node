<div class = "container">
	<div class = "row">
		<div class = "col-lg-4 col-md-4 col-sm-4 col-xs-4 col-lg-offset-4 col-md-offset-4 col-sm-offset-4 col-xs-offset-4" id = "result">
			{{#if patient.checked}}
				<h2 id = "result-message">You have been checked in. You can sit down and we will call you when the doctor is ready.</h2>
				<div class = "text-center">
					<a href = "/" id = "result-confirmation">OK, thank you.</a>
				</div>
			{{else}}
				<h2 id = "result-message">You have not been checked in. Please try again or contact a staff member for assitance.</h2>
				<div class = "text-center">
					<a href = "/" id = "result-confirmation">Start over</a>
				</div>
			{{/if}}
		</div>
	</div>
</div>